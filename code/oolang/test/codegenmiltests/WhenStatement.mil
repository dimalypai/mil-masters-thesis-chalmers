type Maybe A
  = Nothing
  | Just A;

type String
  = Empty_Str
  | Cons_Str Char String;

printString : String -> (State ::: (Error Unit ::: IO)) Unit =
  \(s_ : String) ->
    case s_ of
      | Empty_Str =>
          return [State ::: (Error Unit ::: IO)]
            unit
      | Cons_Str (c_ : Char) (cs_ : String) =>
          let (unit_0 : Unit) <-
            lift [IO => State ::: (Error Unit ::: IO)] print_char c_
          in printString cs_
    end;

readString : (State ::: (Error Unit ::: IO)) String =
  readString_ Empty_Str;

readString_ : String -> (State ::: (Error Unit ::: IO)) String =
  \(acc_ : String) ->
    let (c_ : Char) <-
      lift [IO => State ::: (Error Unit ::: IO)] read_char
    in case c_ of
         | ' ' =>
             return [State ::: (Error Unit ::: IO)]
               reverseString_ acc_ Empty_Str
         | _ =>
             readString_ (Cons_Str c_ acc_)
       end;

reverseString_ : String -> String -> String =
  \(s_ : String) ->
    \(acc_ : String) ->
      case s_ of
        | Empty_Str =>
            acc_
        | Cons_Str (c_ : Char) (cs_ : String) =>
            reverseString_ cs_ (Cons_Str c_ acc_)
      end;

printBool : Bool -> (State ::: (Error Unit ::: IO)) Unit =
  \(b_ : Bool) ->
    case b_ of
      | True =>
          printString (Cons_Str 't' (Cons_Str 'r' (Cons_Str 'u' (Cons_Str 'e' Empty_Str))))
      | False =>
          printString (Cons_Str 'f' (Cons_Str 'a' (Cons_Str 'l' (Cons_Str 's' (Cons_Str 'e' Empty_Str)))))
    end;

readBool : (State ::: (Error Unit ::: IO)) Bool =
  let (c_1 : Char) <-
    lift [IO => State ::: (Error Unit ::: IO)] read_char
  in case c_1 of
       | 't' =>
           let (c_2 : Char) <-
             lift [IO => State ::: (Error Unit ::: IO)] read_char
           in case c_2 of
                | 'r' =>
                    let (c_3 : Char) <-
                      lift [IO => State ::: (Error Unit ::: IO)] read_char
                    in case c_3 of
                         | 'u' =>
                             let (c_4 : Char) <-
                               lift [IO => State ::: (Error Unit ::: IO)] read_char
                             in case c_4 of
                                  | 'e' =>
                                      let (c_5 : Char) <-
                                        lift [IO => State ::: (Error Unit ::: IO)] read_char
                                      in case c_5 of
                                           | ' ' =>
                                               return [State ::: (Error Unit ::: IO)]
                                                 True
                                           | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                                         end
                                  | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                                end
                         | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                       end
                | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
              end
       | 'f' =>
           let (c_2 : Char) <-
             lift [IO => State ::: (Error Unit ::: IO)] read_char
           in case c_2 of
                | 'a' =>
                    let (c_3 : Char) <-
                      lift [IO => State ::: (Error Unit ::: IO)] read_char
                    in case c_3 of
                         | 'l' =>
                             let (c_4 : Char) <-
                               lift [IO => State ::: (Error Unit ::: IO)] read_char
                             in case c_4 of
                                  | 's' =>
                                      let (c_5 : Char) <-
                                        lift [IO => State ::: (Error Unit ::: IO)] read_char
                                      in case c_5 of
                                           | 'e' =>
                                               let (c_6 : Char) <-
                                                 lift [IO => State ::: (Error Unit ::: IO)] read_char
                                               in case c_6 of
                                                    | ' ' =>
                                                        return [State ::: (Error Unit ::: IO)]
                                                          False
                                                    | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                                                  end
                                           | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                                         end
                                  | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                                end
                         | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                       end
                | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
              end
       | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
     end;

printInt : Int -> (State ::: (Error Unit ::: IO)) Unit =
  \(i_ : Int) ->
    return [State ::: (Error Unit ::: IO)]
      unit;

readInt : (State ::: (Error Unit ::: IO)) Int =
  return [State ::: (Error Unit ::: IO)]
    1;

printFloat : Float -> (State ::: (Error Unit ::: IO)) Unit =
  \(f_ : Float) ->
    return [State ::: (Error Unit ::: IO)]
      unit;

readFloat : (State ::: (Error Unit ::: IO)) Float =
  return [State ::: (Error Unit ::: IO)]
    1.0;

main : (State ::: (Error Unit ::: IO)) Unit =
  let (var_1 : Unit) <-
    let (var_0 : Bool) <-
      return [State ::: (Error Unit ::: IO)]
        True
    in case var_0 of
         | True =>
             return [State ::: (Error Unit ::: IO)]
               unit
         | False =>
             return [State ::: (Error Unit ::: IO)]
               unit
       end
  in let (var_3 : Int) <-
       let (var_2 : Bool) <-
         return [State ::: (Error Unit ::: IO)]
           False
       in case var_2 of
            | True =>
                return [State ::: (Error Unit ::: IO)]
                  1
            | False =>
                return [State ::: (Error Unit ::: IO)]
                  2
          end
     in return [State ::: (Error Unit ::: IO)]
          unit;

