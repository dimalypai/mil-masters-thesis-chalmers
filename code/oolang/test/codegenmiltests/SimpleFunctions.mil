type Bool
  = True
  | False;

type Maybe A
  = Nothing
  | Just A;

alias Impure_M = Error Unit ::: State ::: Lift ::: IO ::: Id;

main : Impure_M Unit =
  let (_ : Unit) <- return [Impure_M] unit in
    let (_ : Int) <- lift [Id -> Impure_M] pureFun in
      main;

pureFun : Id Int =
  let (_ : Unit) <- return [Id] unit in
    let (_ : Int) <- pureFun in
      return [Id] 1;

