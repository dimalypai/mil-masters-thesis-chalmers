type Maybe A
  = Nothing
  | Just A;

alias Pure_M = Error Unit ::: NonTerm;

alias Impure_M = Error Unit ::: NonTerm ::: State ::: IO;

main : Impure_M Unit =
  let (_ : Unit) <- return [Impure_M] unit in
    let (_ : Int) <- pureFun in
      main;

pureFun : Pure_M Int =
  let (_ : Unit) <- return [Pure_M] unit in
    let (_ : Int) <- pureFun in
      return [Pure_M] 1;

