           in case c_2 of
                | 'a' =>
                    let (c_3 : Char) <-
                      lift [IO => State ::: (Error Unit ::: IO)] read_char
                    in case c_3 of
                         | 'l' =>
                             let (c_4 : Char) <-
                               lift [IO => State ::: (Error Unit ::: IO)] read_char
                             in case c_4 of
                                  | 's' =>
                                      let (c_5 : Char) <-
                                        lift [IO => State ::: (Error Unit ::: IO)] read_char
                                      in case c_5 of
                                           | 'e' =>
                                               let (c_6 : Char) <-
                                                 lift [IO => State ::: (Error Unit ::: IO)] read_char
                                               in case c_6 of
                                                    | ' ' =>
                                                        return [State ::: (Error Unit ::: IO)]
                                                          False
                                                    | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                                                  end
                                           | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                                         end
                                  | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                                end
                         | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
                       end
                | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
              end
       | _ => lift [Error Unit => State ::: Error Unit] throw_error [Unit] [Bool] unit
     end;

