main : IO Unit
main = do
  printString "Hello from main";
  i : Int <- readInt;
  printInt i;
  f : Float <- readFloat;
  printFloat f;
  printInt (execState [Int] [Unit] stateManip 1);
  return (evalState [Int] [Unit] stateManip 0);
end;;

stateManip : State Int Unit
stateManip = do
  i : Int <- get [Int];
  put [Int] i;
  modify [Int] (\(s : Int) -> s);
end;;

